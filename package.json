{
  "name": "vibecoderai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run build:tailwind && vite",
    "build:tailwind": "tailwindcss -c tailwind.config.cjs -i tailwind.input.css -o public/tailwind.css --minify",
    "build": "npm run build:tailwind && vite build && node scripts/generate-sitemap.mjs",
    "build:prerender": "npm run build:tailwind && PRERENDER=1 vite build && node scripts/generate-sitemap.mjs",
    "postinstall": "node scripts/patch-prerenderer-host.mjs",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "^1.33.0",
    "@supabase/supabase-js": "^2.88.0",
    "lucide-react": "^0.561.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@vitejs/plugin-react": "^5.0.0",
    "puppeteer": "^22.15.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-prerender": "^1.0.8"
  },
  "overrides": {
    "puppeteer": "^22.15.0"
  }
}
